{
  "swagger" : "2.0",
  "info" : {
    "description" : "Home meassurements test app By Marton Szatzker",
    "version" : "v1",
    "title" : "Measapp API",
    "termsOfService" : "http://www.github.com/kongchen/swagger-maven-plugin",
    "contact" : {
      "name" : "marton.szatzker",
      "url" : "http://tzktechnologies.com",
      "email" : "marton.szatzker@gmail.com"
    },
    "license" : {
      "name" : "Apache 2.0",
      "url" : "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "host" : "localhost:8090",
  "basePath" : "/api",
  "schemes" : [ "http" ],
  "paths" : {
    "/api/user" : {
      "post" : {
        "operationId" : "addTestUser",
        "responses" : {
          "default" : {
            "description" : "successful operation"
          }
        }
      }
    },
    "/api/user/showMeSimpleTestUser" : {
      "get" : {
        "summary" : "Returns all test user",
        "description" : "",
        "operationId" : "simpleTestCall",
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/UserData"
              }
            }
          }
        }
      }
    },
    "/api/user/showMeTestUser" : {
      "get" : {
        "operationId" : "testCall",
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "$ref" : "#/definitions/PagingResponseUserData"
            }
          }
        }
      }
    }
  },
  "securityDefinitions" : {
    "basicAuth" : {
      "type" : "basic"
    }
  },
  "definitions" : {
    "PagingResponse" : {
      "type" : "object",
      "properties" : {
        "totalCount" : {
          "type" : "integer",
          "format" : "int64"
        },
        "result" : {
          "type" : "array",
          "items" : {
            "type" : "object"
          }
        }
      }
    },
    "PagingResponseUserData" : {
      "type" : "object",
      "properties" : {
        "totalCount" : {
          "type" : "integer",
          "format" : "int64"
        },
        "result" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/definitions/UserData"
          }
        }
      }
    },
    "UserData" : {
      "type" : "object",
      "properties" : {
        "userName" : {
          "type" : "string"
        },
        "emailAddress" : {
          "type" : "string"
        },
        "location" : {
          "type" : "string"
        },
        "admin" : {
          "type" : "boolean"
        },
        "executor" : {
          "type" : "boolean"
        }
      },
      "description" : "Api test model for users"
    }
  }
}